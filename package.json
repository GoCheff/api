{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "setup": "ts-node src/application/executables/createEnvFiles.ts",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "start": "node ./dist/index.js",
    "dev": "npm run dev:load-envs npx ts-node-dev -- src/index.ts -e ts",
    "dev:prisma:studio": "npm run dev:load-envs npx prisma studio",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev:generate": "npm run dev:load-envs prisma generate",
    "dev:migrate": "npm run dev:load-envs prisma migrate dev",
    "dev:load-envs": "dotenv -e .env.development",
    "test": "npm run test:load-envs jest -- -- -i --passWithNoTests --coverage --verbose",
    "test:int": "npm run test -- --testPathPattern=integration",
    "test:unit": "npm run test -- --testPathPattern=unit",
    "test:int:watch": "npm run test:int -- --watch",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:generate": "npm run test:load-envs prisma generate",
    "test:migrate": "npm run test:load-envs prisma migrate dev",
    "test:load-envs": "dotenv -e .env.test"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.3",
    "joi-translation-pt-br": "^0.0.6",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.13.0",
    "swagger-ui-express": "^4.6.3",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@mermaid-js/mermaid-cli": "^9.1.7",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^6.6.6",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "prisma-erd-generator": "^1.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  }
}
